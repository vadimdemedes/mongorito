{
  "name": "mongorito",
  "version": "2.2.0",
  "description": "ES6 generator-based MongoDB ODM. It rocks.",
  "author": "Vadim Demedes <vdemedes@gmail.com>",
  "dependencies": {
    "mongodb": "^2.2.9"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "chance": "^1.0.4",
    "coveralls": "^2.11.12",
    "eslint-config-vdemedes": "^1.0.2",
    "nyc": "^8.1.0",
    "xo": "^0.16.0"
  },
  "files": [
    "index.js",
    "util",
    "lib"
  ],
  "scripts": {
    "test": "xo && nyc ava --serial",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": "vdemedes/mongorito",
  "keywords": [
    "mongo",
    "mongodb",
    "co-mongo",
    "co-mongodb",
    "orm",
    "odm",
    "es6"
  ],
  "license": "MIT",
  "xo": {
    "extends": "vdemedes",
    "esnext": true,
    "env": [
      "node",
      "mocha"
    ],
    "ignore": [
      "examples/*.js"
    ],
    "rules": {
      "prefer-arrow-callback": 0,
      "prefer-spread": 0,
      "no-use-extend-native/no-use-extend-native": 0
    }
  }
}
