{
  "name": "mongorito",
  "version": "2.2.0",
  "description": "ES6 generator-based MongoDB ODM. It rocks.",
  "author": "Vadim Demedes <vdemedes@gmail.com>",
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "arrify": "^1.0.1",
    "dot-prop": "^4.0.0",
    "is-plain-obj": "^1.1.0",
    "lodash.result": "^4.5.2",
    "mongodb": "^2.2.9",
    "mquery": "^1.11.0",
    "pluralize": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "chance": "^1.0.4",
    "coveralls": "^2.11.12",
    "nyc": "^8.1.0",
    "xo": "^0.16.0"
  },
  "files": [
    "index.js",
    "util",
    "lib"
  ],
  "scripts": {
    "test": "xo && nyc ava --serial",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": "vdemedes/mongorito",
  "keywords": [
    "mongo",
    "mongodb",
    "co-mongo",
    "co-mongodb",
    "orm",
    "odm",
    "es6"
  ],
  "license": "MIT",
  "xo": {
    "esnext": true,
    "env": "node",
    "rules": {
      "array-callback-return": 1
    }
  },
  "ava": {
    "serial": true,
    "failFast": true
  }
}
